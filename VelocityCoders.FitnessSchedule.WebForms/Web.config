<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <appSettings> 
    <add key="ConnectionStringName" value="FitnessScheduleWebApp" />
    <add key="LookupTablesNavigationItems" value="Entity,EntityType"/>
  </appSettings>
  <connectionStrings>
    <add name="FitnessScheduleWebApp"
         connectionString="Data Source=LAPTOP-90DM64R1;Initial Catalog=FitnessSchedule;user id=FitnessScheduleWebUser;password=VelocityCoders15"
         providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2"/>
    <httpRuntime targetFramework="4.5.2"/>
    <customErrors mode="RemoteOnly" defaultRedirect="~/Error.aspx" />
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web"/>
    </httpModules>
